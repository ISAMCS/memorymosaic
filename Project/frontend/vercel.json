{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/model/(.*)", "dest": "api/models.js" },
    { "src": "/(.*)", "dest": "$1" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}