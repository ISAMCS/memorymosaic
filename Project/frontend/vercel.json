{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/next" },
    { "src": "api/server.js", "use": "@vercel/node" },
    { "src": "api/models.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/server.js" },
    { "src": "/model/(.*)", "dest": "api/models.js" },
    { "src": "/(.*)", "dest": "$1" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}